{
  "name": "v5poc",
  "version": "1.0.0",
  "description": "JS SDK v5 POC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .ts",
    "build": "npm run clean; tsc -b tsconfig.cjs.json tsconfig.esm.json tsconfig.esm5.json tsconfig.types.json",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "author": "Rai Phairow",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.2.4",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testRegex": "\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  }
}
